<h1>Login</h1>

<%= form_with url: login_path do |form| %>
  <div>
    <%= form.label :email_address %>
    <%= form.text_field :email_address %>
  </div>

  <div>
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>

  <div>
    <%= form.submit "Login" %>
  </div>
<% end %>
